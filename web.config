<rewrite>
  <rules>
    <rule name="Rewrite to index.php">
      <match url="^([_0-9a-z-]+)" />
      <action type="Rewrite" url="index.php?cat={R:1}" />
    </rule>
    <rule name="Rewrite resume">
      <match url="^resume" />
      <action type="Rewrite" url="bin/resume.pdf" />
    </rule>
  </rules>
</rewrite>